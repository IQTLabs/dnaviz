{
  "name": "wasm-benchmarking",
  "version": "1.0.0",
  "description": "Benchmarking Webassembly versus Javascript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap -O3 --noAssert",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@assemblyscript/loader": "^0.13.3"
  },
  "devDependencies": {
    "assemblyscript": "^0.13.3",
    "benchmark": "^2.1.4",
    "dnaviz": "^1.0.0-0"
  }
}
