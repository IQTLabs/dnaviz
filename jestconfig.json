{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "globals": {"ts-jest": {"tsConfig": "tsconfig.json"}},
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
}